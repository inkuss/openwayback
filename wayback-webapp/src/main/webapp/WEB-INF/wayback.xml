<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-init-method="init">

<!--
    Macro-like substitutions for the overall file:
      wayback.basedir:       default top level directory for all index, state, locationdb storage for the restricted access collection ("Lesesaal").
      wayback.publicbasedir: default top level directory for all index, state, locationdb storage for the public  collection.
-->

  <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
    <property name="properties">
      <value>
        <!-- Customize these basic placeholders. -->
        wayback.basedir.default=/opt/regal/openwayback-data/lesesaal
        wayback.alternatebasedir.default=/opt/regal/openwayback-data/alternate
        wayback.publicbasedir.default=/opt/regal/openwayback-data/weltweit
        wayback.url.scheme.default=http
        wayback.url.host.default=edoweb-rlp.de
        wayback.url.port.default=9080

        <!-- Environment variables (if present) override defaults. No need to customize these. -->
        wayback.basedir=#{ systemEnvironment['WAYBACK_BASEDIR'] ?: '${wayback.basedir.default}' }
        wayback.alternatebasedir=#{ systemEnvironment['WAYBACK_ALTERNATEBASEDIR'] ?: '${wayback.alternatebasedir.default}' }
        wayback.publicbasedir=#{ systemEnvironment['WAYBACK_PUBLICBASEDIR'] ?: '${wayback.publicbasedir.default}' }
        wayback.url.scheme=#{ systemEnvironment['WAYBACK_URL_SCHEME'] ?: '${wayback.url.scheme.default}' }
        wayback.url.host=#{ systemEnvironment['WAYBACK_URL_HOST'] ?: '${wayback.url.host.default}' }
        wayback.url.port=#{ systemEnvironment['WAYBACK_URL_PORT'] ?: '${wayback.url.port.default}' }

        <!-- No need to edit this setting unless deploying in a non-ROOT context
             or using a load balancer, in which case configure it with the frontend URL prefix. -->
        <!--wayback.url.prefix.default=${wayback.url.scheme}://${wayback.url.host}:${wayback.url.port}
-->
	wayback.url.prefix.default=https://api.${wayback.url.host.default}

        <!-- Environment variable (if present) overrides default. No need to customize this. -->
        wayback.url.prefix=#{ systemEnvironment['WAYBACK_URL_PREFIX'] ?: '${wayback.url.prefix.default}' }

        <!-- Customize or add additional placeholders if needed to use elsewhere.
             Check BDBCollection.xml for following placeholders being used. -->
        wayback.archivedir.1=/opt/regal/wpull-data/
        wayback.archivedir.2=/opt/regal/heritrix-data/
        wayback.archivedir.3=/opt/regal/cdn-data/
        <!-- wget-data : migration data from edoweb2 -->
        wayback.archivedir.4=/opt/regal/wget-data/
        wayback.archivedir.5=/opt/regal/public-data/
      </value>
    </property>
  </bean>

  <bean id="waybackCanonicalizer" class="org.archive.wayback.util.url.AggressiveUrlCanonicalizer" />

<!--
	The XML files indicated in the following import tags contain implementations of WaybackCollections. 
        BDBCollection.xml is the standard wayback collection, used for restricted access material ("Lesesaal") in the HBZ Edoweb implementation.
        BDBCollectionPublic.xml is an additional wayback collection for HBZ Edoweb which is used for public access archives only.
 
        The order of imports must not be changed.
-->
  <import resource="BDBCollection.xml"/>
  <import resource="BDBCollectionAlternate.xml"/>
  <import resource="BDBCollectionPublic.xml"/>

<!--

  <import resource="CDXCollection.xml"/>
  <import resource="RemoteCollection.xml"/>
  <import resource="NutchCollection.xml"/>
-->




